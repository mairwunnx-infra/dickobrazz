{
  "uid": "dickobrazz",
  "title": "Dickobrazz Application Metrics",
  "tags": [
    "dickobrazz"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Total Users",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "dickobrazz_total_users",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "values": false
        },
        "orientation": "horizontal"
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "DAU (24h)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 6,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "dickobrazz_stats_dau",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "values": false
        },
        "orientation": "horizontal"
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "MAU (30d)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "dickobrazz_stats_mau",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "values": false
        },
        "orientation": "horizontal"
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Uptime (s)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 18,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "dickobrazz_uptime_seconds",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "values": false
        },
        "orientation": "horizontal"
      }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Availability (%)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 6,
        "h": 4
      },
      "fieldConfig": {
        "defaults": {
          "decimals": 2,
          "unit": "percent"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "dickobrazz_availability_percent",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "values": false
        },
        "orientation": "horizontal"
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Handled Updates",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 6,
        "y": 4,
        "w": 9,
        "h": 6
      },
      "targets": [
        {
          "expr": "sum by (status) (increase(dickobrazz_messages_handled_total[$__rate_interval]))",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Ignored Updates",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 15,
        "y": 4,
        "w": 9,
        "h": 6
      },
      "targets": [
        {
          "expr": "sum by (reason) (increase(dickobrazz_messages_ignored_total[$__rate_interval]))",
          "legendFormat": "{{reason}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Update Duration p95",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 0,
        "y": 10,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, type) (rate(dickobrazz_update_duration_seconds_bucket[5m])))",
          "legendFormat": "{{type}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "Detected Languages",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 12,
        "y": 10,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "sum by (language) (increase(dickobrazz_detected_languages_total[$__rate_interval]))",
          "legendFormat": "{{language}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 10,
      "type": "barchart",
      "title": "Size Distribution",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 24,
        "h": 6
      },
      "targets": [
        {
          "expr": "dickobrazz_size_distribution",
          "instant": true,
          "legendFormat": "{{bucket}}",
          "range": false,
          "refId": "A"
        }
      ]
    }
  ],
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  }
}
