TZ=Europe/Moscow
GF_SECURITY_ADMIN_USER=# логин администратора Grafana
GF_SECURITY_ADMIN_PASSWORD=# пароль администратора Grafana

BOT_TG_TOKEN=# токен production-бота из @BotFather
TEST_BOT_TG_TOKEN=# токен test-бота из @BotFather

CROSS_SERVER_TOKEN=# общий CSOT-токен для production bot/server
TEST_CROSS_SERVER_TOKEN=# общий CSOT-токен для test bot/server

SESSION_SECRET=# production secret для подписи сессий сервера
TEST_SESSION_SECRET=# test secret для подписи сессий сервера
SESSION_TTL_SEC=604800

MONGO_URL=# строка подключения production mongo (mongodb://...)
REDIS_URL=# строка подключения production redis (redis://...)
TEST_MONGO_URL=# строка подключения test mongo (mongodb://...)
TEST_REDIS_URL=# строка подключения test redis (redis://...)

MONGO_INITDB_DATABASE=dickobrazz
MONGO_INITDB_ROOT_USERNAME=# root пользователь production mongo
MONGO_INITDB_ROOT_PASSWORD=# root пароль production mongo
TEST_MONGO_INITDB_ROOT_USERNAME=# root пользователь test mongo
TEST_MONGO_INITDB_ROOT_PASSWORD=# root пароль test mongo

REDIS_PASSWORD=# пароль production redis
TEST_REDIS_PASSWORD=# пароль test redis

RANDOMORG_TOKEN=# API токен random.org (получить в личном кабинете random.org)

AWS_ENDPOINT=# endpoint S3-совместимого хранилища (например s3.timeweb.com)
AWS_ENDPOINT_PROTO=https
AWS_S3_BUCKET_NAME=mairwunnx.backups
AWS_ACCESS_KEY_ID=# Access Key для S3-совместимого хранилища
AWS_SECRET_ACCESS_KEY=# Secret Key для S3-совместимого хранилища

BACKUP_CRON_EXPRESSION=0 3 * * *
BACKUP_RETENTION_DAYS=5
STACK_SLUG=dickobrazz
